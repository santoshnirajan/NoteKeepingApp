      "post": {
        "tags": [
          "Files"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  },
                  "altText": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "createdBy": {
                    "type": "string",
                    "format": "uuid"
                  }
                }
              },
              "encoding": {
                "file": {
                  "style": "form"
                },
                "altText": {
                  "style": "form"
                },
                "description": {
                  "style": "form"
                },
                "createdBy": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
